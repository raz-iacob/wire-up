includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon
    - phar://phpstan.phar/conf/bleedingEdge.neon

parameters:

    paths:
        - app
        - bootstrap/app.php
        - config
        - database
        - public
        - routes

    level: max

    tmpDir: /tmp/phpstan

    ignoreErrors:
        -   # Livewire route macro
            message: '#Cannot call method name\(\) on mixed\.#'
            path: routes

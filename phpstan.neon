includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon
    - phar://phpstan.phar/conf/bleedingEdge.neon

parameters:

    paths:
        - app
        - bootstrap/app.php
        - config
        - database
        - public
        - routes
        - resources/views

    level: 6

    tmpDir: /tmp/phpstan

    ignoreErrors:
        - '#Call to an undefined method Livewire\\Component@anonymous.+::view#'